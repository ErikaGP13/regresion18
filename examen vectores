#ejercicio 5
facultad <-c("ECONOMIA","INGENIERIA","ARQUITECTURA","DERECHO")
licenciatura <-c("ACTUARIA","INGENIERIA CIVIL","ARQUITECTURA","DERECHO")
calificacion<-c("8","8","9","10")
mode(facultad)
mode(licenciatura)
mode(calificacion)
#renglon facultad
dataf<-data.frame(facultad,licenciatura,calificacion)
dataf
dataf[-3,]

####6
EDAD<-c("32","35","43","25")
NOMBRE<-c("carlos","juan","raquel","paty")
SITU.CONYUGAL<-c("casado","soltero","soltero","union libre")
SEGURIDAD_SOCIAL<-c("imss","imss","issste","privado")
OCUPACION <-c("contador","carpintero","profesora","diseñador")
dataf2 <-data.frame(EDAD,NOMBRE,SITU.CONYUGAL,SEGURIDAD_SOCIAL,OCUPACION)
dataf2  

dataf2()
OCUPACION[c(1,4)]
SEGURIDAD_SOCIAL[c(3,1)]
SITU.CONYUGAL[c(4,2)]
x<-sum(dataf2[,1])
x
