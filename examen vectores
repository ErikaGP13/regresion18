#crea una matriz con la funcion dim() de 5 columnas y 4 rebglones
#########ejercicio 1
matriz1<-matrix(11:30,nrow = 4,ncol = 5)
matriz1
dim(matriz1)
rownames(matriz1) <-c("toluca","pachuca","guadalajara","monterrey")
colnames(matriz1) <-c("escuela1","escuela2","escuela3","escuela4","escuela5")
matriz1
matriz1[1,3]
matriz1[3,5]
matriz1[2,1]

#ejercicio 2
